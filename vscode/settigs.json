  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "swaggerViewer.previewInBrowser": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "python.jediEnabled": false,
  "go.formatTool": "goimports",
  "go.useLanguageServer": true,
  "files.exclude": {
    ".history": true,
    ".idea": true,
    "**/__pycache__": true,
    "**/bin": true,
    "**/obj": true,
    "node_modules": true
  },
  "vim.foldfix": true,
  "vim.scroll": 5,
  "vim.vimrc.path": "/Users/vdoroshenko/.vimrc",
  "vim.normalModeKeyBindings": [
    {
      "before": ["<C-j>"],
      "after": ["<CR><ESC>"]
    }
  ],
  "python.languageServer": "Pylance",
  "material-icon-theme.folders.theme": "specific",
  "material-icon-theme.folders.color": "#42a5f5",
  "materialTheme.accent": "Remove accents",
  "vim.camelCaseMotion.enable": true,
  "vim.cursorStylePerMode.insert": "line",
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.visual": "underline",
  "vim.vimrc.enable": true,
  "editor.lineNumbers": "relative",
  "vim.commandLineModeKeyBindings": [],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["<C-e>"],
      "after": ["<C-e>", "<C-e>", "<C-e>", "<C-e>", "<C-e>"]
    },
    {
      "before": ["<C-y>"],
      "after": ["<C-y>", "<C-y>", "<C-y>", "<C-y>", "<C-y>"]
    }
  ],
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "csharp.maxProjectFileCountForDiagnosticAnalysis": 100,
  "csharp.semanticHighlighting.enabled": true,
  "omnisharp.maxFindSymbolsItems": 10,
  "omnisharp.maxProjectResults": 10,
  "omnisharp.enableEditorConfigSupport": true,
  "omnisharp.enableImportCompletion": true,
  "omnisharp.enableRoslynAnalyzers": true,
  "omnisharp.path": "latest",
  "omnisharp.useGlobalMono": "never",
  "omnisharp.minFindSymbolsFilterLength": 3,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.pinnedTabSizing": "shrink",
  "workbench.editor.showTabs": false,
  "workbench.fontAliasing": "antialiased",
  "workbench.tree.renderIndentGuides": "always",
  "editor.renderControlCharacters": true,
  "debug.onTaskErrors": "showErrors",
  "python.pythonPath": "/usr/local/opt/python@3.8/bin/python3.8",
  "todo-tree.tree.showScanModeButton": false,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "markdown.preview.breaks": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "gitlens.defaultDateFormat": null,
  "git.autofetch": true,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "editor.renderWhitespace": "all",
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "go.toolsManagement.autoUpdate": true,
  "redhat.telemetry.enabled": false,
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "rust-client.rustupPath": "~/.cargo/bin/rustup",
  "[rust]": {
    "editor.defaultFormatter": "matklad.rust-analyzer"
  },
  "liveServer.settings.donotShowInfoMsg": true,
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "omnisharp.loggingLevel": "warning",
  "editor.cursorStyle": "line",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.tabCompletion": "on",
  "editor.formatOnType": true,
  "diffEditor.wordWrap": "on",
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.insertSpaces": true,
  "python.defaultInterpreterPath": "/usr/local/opt/python@3.8/bin/python3.8",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "omnisharp.organizeImportsOnFormat": true,
  "hediet.vscode-drawio.local-storage": "",
  "omnisharp.enableDecompilationSupport": true,
  "omnisharp.enableAsyncCompletion": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "workbench.colorTheme": "Visual Studio Light",
  "coverage-gutters.showRulerCoverage": true,
  "coverage-gutters.showLineCoverage": true,
  "coverage-gutters.showGutterCoverage": false,
  "editor.fontSize": 14,
  "scm.inputFontSize": 12,
  "terminal.integrated.fontSize": 13,
  "editor.inlayHints.fontSize": 8,
  "debug.terminal.clearBeforeReusing": true,
  "terminal.external.osxExec": "iTerm2.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.defaultProfile.osx": "fish",
}
